@{
    ViewData["Title"] = "Data";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Data</title>
    <link rel="stylesheet" href="~/css/sites.css">
</head>
<body>
    <header>
        <nav>
            <div class="container">
                <h1 class="logo">Travel Blog</h1>
                <ul class="nav-links">
                    <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                    <li><a href="@Url.Action("Index", "Blog")">Blog</a></li>
                    <li><a href="@Url.Action("Index", "Data")">Data</a></li>
                    <li><a href="@Url.Action("Index", "About")">About</a></li>
                </ul>
            </div>
        </nav>
    </header>
    <section class="database">
        <div class="container">
            <h2>Likes Data</h2>
            <table border="1">
                <tr>
                    @foreach (var key in ((List<Dictionary<string, string>>)ViewData["Likes"]).First().Keys)
                    {
                        <th>@key</th>
                    }
                </tr>
                @foreach (var item in (List<Dictionary<string, string>>)ViewData["Likes"])
                {
                    <tr>
                        @foreach (var value in item.Values)
                        {
                            <td>@value</td>
                        }
                    </tr>
                }
            </table>
            <h2>Visitors Data</h2>
            <table border="1">
                <tr>
                    @foreach (var key in ((List<Dictionary<string, string>>)ViewData["Visitors"]).First().Keys)
                    {
                        <th>@key</th>
                    }
                </tr>
                @foreach (var item in (List<Dictionary<string, string>>)ViewData["Visitors"])
                {
                    <tr>
                        @foreach (var value in item.Values)
                        {
                            <td>@value</td>
                        }
                    </tr>
                }
            </table>
            <h2>Comments Data</h2>
            <table border="1">
                <tr>
                    @foreach (var key in ((List<Dictionary<string, string>>)ViewData["Comments"]).First().Keys)
                    {
                        <th>@key</th>
                    }
                </tr>
                @foreach (var item in (List<Dictionary<string, string>>)ViewData["Comments"])
                {
                    <tr>
                        @foreach (var value in item.Values)
                        {
                            <td>@value</td>
                        }
                    </tr>
                }
            </table>
        </div>
    </section>
    <script src="~/js/script.js"></script>
</body>
</html>
